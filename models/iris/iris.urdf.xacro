<robot name="iris" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:arg name='mavlink_udp_port' default='14560' />
  <xacro:arg name='mavlink_tcp_port' default='4560' />
  <xacro:arg name='qgc_udp_port' default='4560' />
  <xacro:arg name='enable_camera' default='true' />
  <xacro:arg name='camera_pose' default='0 0 0 0 0 0' />
  <xacro:arg name='camera_udp_port' default='5600' />
  <xacro:arg name='rotors_description_dir' default='.'/>
  <xacro:arg name='vehicle_name' default='iris'/>
    <link name='$(arg vehicle_name)_base_link'>
      <pose rpy="0  0  0" xyz="0  0  0"/>
      <inertial>
        <mass value="1.5"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="0.0347563" ixy="0" ixz="0" iyy="0.0458929" iyz="0" izz="0.0977"/>
      </inertial>
      <collision name="$(arg vehicle_name)_base_link_inertia_collision">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <box size="0.47 0.47 0.11"/>
        </geometry>
      </collision>
      <visual name="$(arg vehicle_name)_base_link_inertia_visual">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <mesh filename="package://mavlink_sitl_gazebo/models/rotors_description/meshes/iris.stl" scale="1 1 1"/>
        </geometry>
        <material name="Gazebo/Black">
          <color rgba="0. 0. 0. 1"/>
        </material>
      </visual>
    </link>
    <gazebo reference="$(arg vehicle_name)_base_link">
      <material>Gazebo/Black</material>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </gazebo>
    <link name='$(arg vehicle_name)_imu_link'>
      <pose rpy="0  0  0" xyz="0  0  0"/>
      <inertial>
        <mass value="0.015"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="1e-05" ixy="0" ixz="0" iyy="1e-05" iyz="0" izz="1e-05"/>
      </inertial>
    </link>
    <joint name='$(arg vehicle_name)_imu_joint' type='revolute'>
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="$(arg vehicle_name)_imu_link"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <axis xyz="1  0  0"/>
      <limit effort="0.0" lower="0.0" upper="0.0" velocity="0.0"/>
    </joint>
    <link name='$(arg vehicle_name)_rotor_0'>
      <pose rpy="0.13 -0.22" xyz="0.023 0 -0 0"/>
      <inertial>
        <mass value="0.005"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="9.75e-07" ixy="0" ixz="0" iyy="0.000273104" iyz="0" izz="0.000274004"/>
      </inertial>
      <collision name="$(arg vehicle_name)_rotor_0_collision">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <cylinder length="0.005" radius="0.128"/>
        </geometry>
      </collision>
      <visual name="$(arg vehicle_name)_rotor_0_visual">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <mesh filename="package://mavlink_sitl_gazebo/models/tiltrotor/meshes/iris_prop_ccw.dae" scale="1 1 1"/>
        </geometry>
        <material name="Gazebo/Blue">
          <color rgba="0. 0. 1. 1"/>
        </material>
      </visual>
    </link>
    <gazebo reference="$(arg vehicle_name)_rotor_0">
      <material>Gazebo/Blue</material>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </gazebo>
    <joint name="$(arg vehicle_name)_rotor_0_joint" type="revolute">
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="$(arg vehicle_name)_rotor_0"/>
      <origin rpy="0  0  0" xyz="0.13  -0.22   0.023"/>
      <axis xyz="0  0  1"/>
      <limit effort="0.0" lower="-1e+16" upper="1e+16" velocity="0.0"/>
    </joint>
    <link name='$(arg vehicle_name)_rotor_1'>
      <pose rpy="-0.13 0.2" xyz="0.023 0 -0 0"/>
      <inertial>
        <mass value="0.005"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="9.75e-07" ixy="0" ixz="0" iyy="0.000273104" iyz="0" izz="0.000274004"/>
      </inertial>
      <collision name="$(arg vehicle_name)_rotor_1_collision">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <cylinder length="0.005" radius="0.128"/>
        </geometry>
      </collision>
      <visual name="$(arg vehicle_name)_rotor_1_visual">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <mesh filename="package://mavlink_sitl_gazebo/models/tiltrotor/meshes/iris_prop_ccw.dae" scale="1 1 1"/>
        </geometry>
        <material name="Gazebo/Black">
          <color rgba="0. 0. 0. 1"/>
        </material>
      </visual>
    </link>
    <gazebo reference="$(arg vehicle_name)_rotor_1">
      <material>Gazebo/Black</material>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </gazebo>
    <joint name="$(arg vehicle_name)_rotor_1_joint" type="revolute">
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="$(arg vehicle_name)_rotor_1"/>
      <origin rpy="0  0  0" xyz="-0.13   0.2    0.023"/>
      <axis xyz="0  0  1"/>
      <limit effort="0.0" lower="-1e+16" upper="1e+16" velocity="0.0"/>
    </joint>
    <link name='$(arg vehicle_name)_rotor_2'>
      <pose rpy="0.13 0.2" xyz="0.023 0 -0 0"/>
      <inertial>
        <mass value="0.005"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="9.75e-07" ixy="0" ixz="0" iyy="0.000273104" iyz="0" izz="0.000274004"/>
      </inertial>
      <collision name="$(arg vehicle_name)_rotor_2_collision">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <cylinder length="0.005" radius="0.128"/>
        </geometry>
      </collision>
      <visual name="$(arg vehicle_name)_rotor_2_visual">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <mesh filename="package://mavlink_sitl_gazebo/models/tiltrotor/meshes/iris_prop_cw.dae" scale="1 1 1"/>
        </geometry>
        <material name="Gazebo/Blue">
          <color rgba="0. 0. 1. 1"/>
        </material>
      </visual>
    </link>
    <gazebo reference="$(arg vehicle_name)_rotor_2">
      <material>Gazebo/Blue</material>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </gazebo>
    <joint name="$(arg vehicle_name)_rotor_2_joint" type="revolute">
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="$(arg vehicle_name)_rotor_2"/>
      <origin rpy="0  0  0" xyz="0.13   0.22   0.023"/>
      <axis xyz="0  0  1"/>
      <limit effort="0.0" lower="-1e+16" upper="1e+16" velocity="0.0"/>
    </joint>
    <link name='$(arg vehicle_name)_rotor_3'>
      <pose rpy="-0.13 -0.2" xyz="0.023 0 -0 0"/>
      <inertial>
        <mass value="0.005"/>
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <inertia ixx="9.75e-07" ixy="0" ixz="0" iyy="0.000273104" iyz="0" izz="0.000274004"/>
      </inertial>
      <collision name="$(arg vehicle_name)_rotor_3_collision">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <cylinder length="0.005" radius="0.128"/>
        </geometry>
      </collision>
      <visual name="$(arg vehicle_name)_rotor_3_visual">
        <origin rpy="0  0  0" xyz="0  0  0"/>
        <geometry>
          <mesh filename="package://mavlink_sitl_gazebo/models/tiltrotor/meshes/iris_prop_cw.dae" scale="1 1 1"/>
        </geometry>
        <material name="Gazebo/Black">
          <color rgba="0. 0. 0. 1"/>
        </material>
      </visual>
    </link>
    <gazebo reference="$(arg vehicle_name)_rotor_3">
      <material>Gazebo/Black</material>
      <gravity>1</gravity>
      <velocity_decay/>
      <self_collide>0</self_collide>
    </gazebo>
    <joint name="$(arg vehicle_name)_rotor_3_joint" type="revolute">
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="$(arg vehicle_name)_rotor_3"/>
      <origin rpy="0  0  0" xyz="-0.13  -0.2    0.023"/>
      <axis xyz="0  0  1"/>
      <limit effort="0.0" lower="-1e+16" upper="1e+16" velocity="0.0"/>
    </joint>

<gazebo>
    <plugin name='multirobot_base_plugin' filename='libgazebo_multirotor_base_plugin.so'>
      <robotNamespace/>
      <linkName>$(arg vehicle_name)_base_link</linkName>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>
    <plugin name='front_right_motor_model' filename='libgazebo_motor_model.so'>
      <robotNamespace/>
      <jointName>$(arg vehicle_name)_rotor_0_joint</jointName>
      <linkName>$(arg vehicle_name)_rotor_0</linkName>
      <turningDirection>ccw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>1100</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.06</momentConstant>
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>0</motorNumber>
      <rotorDragCoefficient>0.000806428</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>/motor_speed/0</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>
    <plugin name='back_left_motor_model' filename='libgazebo_motor_model.so'>
      <robotNamespace/>
      <jointName>$(arg vehicle_name)_rotor_1_joint</jointName>
      <linkName>$(arg vehicle_name)_rotor_1</linkName>
      <turningDirection>ccw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>1100</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.06</momentConstant>
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>1</motorNumber>
      <rotorDragCoefficient>0.000806428</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>/motor_speed/1</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>
    <plugin name='front_left_motor_model' filename='libgazebo_motor_model.so'>
      <robotNamespace/>
      <jointName>$(arg vehicle_name)_rotor_2_joint</jointName>
      <linkName>$(arg vehicle_name)_rotor_2</linkName>
      <turningDirection>cw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>1100</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.06</momentConstant>
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>2</motorNumber>
      <rotorDragCoefficient>0.000806428</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>/motor_speed/2</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>
    <plugin name='back_right_motor_model' filename='libgazebo_motor_model.so'>
      <robotNamespace/>
      <jointName>$(arg vehicle_name)_rotor_3_joint</jointName>
      <linkName>$(arg vehicle_name)_rotor_3</linkName>
      <turningDirection>cw</turningDirection>
      <timeConstantUp>0.0125</timeConstantUp>
      <timeConstantDown>0.025</timeConstantDown>
      <maxRotVelocity>1100</maxRotVelocity>
      <motorConstant>8.54858e-06</motorConstant>
      <momentConstant>0.06</momentConstant>
      <commandSubTopic>/gazebo/command/motor_speed</commandSubTopic>
      <motorNumber>3</motorNumber>
      <rotorDragCoefficient>0.000806428</rotorDragCoefficient>
      <rollingMomentCoefficient>1e-06</rollingMomentCoefficient>
      <motorSpeedPubTopic>/motor_speed/3</motorSpeedPubTopic>
      <rotorVelocitySlowdownSim>10</rotorVelocitySlowdownSim>
    </plugin>
    <plugin name='gps_plugin' filename='libgazebo_gps_plugin.so'>
      <robotNamespace/>
      <gpsNoise>1</gpsNoise>
    </plugin>
    <plugin name='magnetometer_plugin' filename='libgazebo_magnetometer_plugin.so'>
      <robotNamespace/>
      <pubRate>20</pubRate>
      <noiseDensity>0.0004</noiseDensity>
      <randomWalk>6.4e-06</randomWalk>
      <biasCorrelationTime>600</biasCorrelationTime>
      <magTopic>/mag</magTopic>
    </plugin>
    <plugin name='barometer_plugin' filename='libgazebo_barometer_plugin.so'>
      <robotNamespace/>
      <pubRate>10</pubRate>
      <baroTopic>/baro</baroTopic>
    </plugin>
    <plugin name='mavlink_interface' filename='libgazebo_mavlink_interface.so'>
      <robotNamespace/>
      <imuSubTopic>/imu</imuSubTopic>
      <gpsSubTopic>/gps</gpsSubTopic>
      <magSubTopic>/mag</magSubTopic>
      <baroSubTopic>/baro</baroSubTopic>
      <mavlink_addr>INADDR_ANY</mavlink_addr>
      <mavlink_udp_port>$(arg mavlink_udp_port)</mavlink_udp_port>
      <mavlink_tcp_port>$(arg mavlink_tcp_port)</mavlink_tcp_port>
      <serialEnabled>false</serialEnabled>
      <serialDevice>/dev/ttyACM0</serialDevice>
      <baudRate>921600</baudRate>
      <qgc_addr>INADDR_ANY</qgc_addr>
      <qgc_udp_port>$(arg qgc_udp_port)</qgc_udp_port>
      <hil_mode>false</hil_mode>
      <hil_state_level>false</hil_state_level>
      <vehicle_is_tailsitter>false</vehicle_is_tailsitter>
      <send_vision_estimation>false</send_vision_estimation>
      <send_odometry>false</send_odometry>
      <enable_lockstep>true</enable_lockstep>
      <use_tcp>true</use_tcp>
      <motorSpeedCommandPubTopic>/gazebo/command/motor_speed</motorSpeedCommandPubTopic>
      <control_channels>
        <channel name='rotor1'>
          <input_index>0</input_index>
          <input_offset>0</input_offset>
          <input_scaling>1000</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>100</zero_position_armed>
          <joint_control_type>velocity</joint_control_type>
        </channel>
        <channel name='rotor2'>
          <input_index>1</input_index>
          <input_offset>0</input_offset>
          <input_scaling>1000</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>100</zero_position_armed>
          <joint_control_type>velocity</joint_control_type>
        </channel>
        <channel name='rotor3'>
          <input_index>2</input_index>
          <input_offset>0</input_offset>
          <input_scaling>1000</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>100</zero_position_armed>
          <joint_control_type>velocity</joint_control_type>
        </channel>
        <channel name='rotor4'>
          <input_index>3</input_index>
          <input_offset>0</input_offset>
          <input_scaling>1000</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>100</zero_position_armed>
          <joint_control_type>velocity</joint_control_type>
        </channel>
        <channel name='rotor5'>
          <input_index>4</input_index>
          <input_offset>1</input_offset>
          <input_scaling>324.6</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_control_type>velocity</joint_control_type>
          <joint_control_pid>
            <p>0.1</p>
            <i>0</i>
            <d>0</d>
            <iMax>0.0</iMax>
            <iMin>0.0</iMin>
            <cmdMax>2</cmdMax>
            <cmdMin>-2</cmdMin>
          </joint_control_pid>
          <joint_name>zephyr_delta_wing::propeller_joint</joint_name>
        </channel>
        <channel name='rotor6'>
          <input_index>5</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.524</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_control_type>position</joint_control_type>
          <joint_name>zephyr_delta_wing::flap_left_joint</joint_name>
          <joint_control_pid>
            <p>10.0</p>
            <i>0</i>
            <d>0</d>
            <iMax>0</iMax>
            <iMin>0</iMin>
            <cmdMax>20</cmdMax>
            <cmdMin>-20</cmdMin>
          </joint_control_pid>
        </channel>
        <channel name='rotor7'>
          <input_index>6</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.524</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_control_type>position</joint_control_type>
          <joint_name>zephyr_delta_wing::flap_right_joint</joint_name>
          <joint_control_pid>
            <p>10.0</p>
            <i>0</i>
            <d>0</d>
            <iMax>0</iMax>
            <iMin>0</iMin>
            <cmdMax>20</cmdMax>
            <cmdMin>-20</cmdMin>
          </joint_control_pid>
        </channel>
        <channel name='rotor8'>
          <input_index>7</input_index>
          <input_offset>0</input_offset>
          <input_scaling>0.524</input_scaling>
          <zero_position_disarmed>0</zero_position_disarmed>
          <zero_position_armed>0</zero_position_armed>
          <joint_control_type>position</joint_control_type>
        </channel>
      </control_channels>
    </plugin>
    <static>0</static>
    <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
      <joint_name>$(arg vehicle_name)::$(arg vehicle_name)_imu_joint</joint_name>
      <joint_name>$(arg vehicle_name)::$(arg vehicle_name)_rotor_0_joint</joint_name>
      <joint_name>$(arg vehicle_name)::$(arg vehicle_name)_rotor_1_joint</joint_name>
      <joint_name>$(arg vehicle_name)::$(arg vehicle_name)_rotor_2_joint</joint_name>
      <joint_name>$(arg vehicle_name)::$(arg vehicle_name)_rotor_3_joint</joint_name>
      <update_rate>100</update_rate>
    </plugin>
    <plugin name='rotors_gazebo_imu_plugin' filename='libgazebo_imu_plugin.so'>
      <robotNamespace/>
      <linkName>$(arg vehicle_name)_imu_link</linkName>
      <imuTopic>/imu</imuTopic>
      <gyroscopeNoiseDensity>0.0003394</gyroscopeNoiseDensity>
      <gyroscopeRandomWalk>3.8785e-05</gyroscopeRandomWalk>
      <gyroscopeBiasCorrelationTime>1000.0</gyroscopeBiasCorrelationTime>
      <gyroscopeTurnOnBiasSigma>0.0087</gyroscopeTurnOnBiasSigma>
      <accelerometerNoiseDensity>0.004</accelerometerNoiseDensity>
      <accelerometerRandomWalk>0.006</accelerometerRandomWalk>
      <accelerometerBiasCorrelationTime>300.0</accelerometerBiasCorrelationTime>
      <accelerometerTurnOnBiasSigma>0.196</accelerometerTurnOnBiasSigma>
    </plugin>
  </gazebo>

  <xacro:if value="$(arg enable_camera)">
    <xacro:include filename="$(arg rotors_description_dir)/fpv_cam/fpv_cam.urdf.xacro">
    </xacro:include>
    <xacro:fpv_cam/>
    <joint name="fpv_joint" type="fixed">
      <parent link="$(arg vehicle_name)_base_link"/>
      <child link="camera_link"/>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <axis xyz="0  0  1"/>
      <limit effort="0.0" lower="0.0" upper="0.0" velocity="0.0"/>
    </joint>
  </xacro:if>
</robot>
